{
  "name": "@brandonnpm2/nestjs-redis",
  "version": "1.1.0-next.0",
  "description": "A NestJS module for using ioredis.",
  "author": "Brandon Tsang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandongit2/nestjs-redis"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts' --fix",
    "publish": "yarn build && yarn npm publish --access public",
    "publish:next": "yarn build && yarn npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.4",
    "ioredis": "^4.27.6"
  },
  "devDependencies": {
    "@nestjs/common": "8.0.4",
    "@types/ioredis": "4.26.6",
    "@types/node": "16.4.1",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "eslint": "7.31.0",
    "eslint-plugin-import": "2.23.4",
    "ioredis": "4.27.6",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
